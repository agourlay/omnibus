<h4>Timers metrics</h4>

<table id="center-table" class="pure-table pure-table-horizontal pure-table-striped">
	<thead>
	    <tr>
	    	<th>name</th>
	        <th>count</th>
	        <th>max</th>
	        <th>min</th>
	        <th>mean</th>
	        <th>stdDev</th>
	        <th>50p</th>
	        <th>75p</th>
	        <th>95p</th>
	        <th>98p</th>
	        <th>99p</th>
	        <th>99.9p</th>
	        <th>15m</th>
	        <th>5m</th>
	        <th>1m</th>
	        <th>mean</th>
	    </tr>
	</thead>
	<tbody>
		{{#each view.content.timers}}
			<tr>
				<td class="left">{{name}}</td>
				<td>{{count}}</td>
				<td>{{duration max 2}}</td>
				<td>{{duration min 2}}</td>
				<td>{{duration mean 2}}</td>
				<td>{{duration stdDev 2}}</td>
				<td>{{duration 50p 2}}</td>
				<td>{{duration 75p 2}}</td>
				<td>{{duration 95p 2}}</td>
				<td>{{duration 98p 2}}</td>
				<td>{{duration 99p 2}}</td>
				<td>{{duration 999p 2}}</td>
				<td>{{toFixed fifteenMinuteRate 2}} / sec</td>
				<td>{{toFixed fiveMinuteRate 2}} / sec</td>
			    <td>{{toFixed oneMinuteRate 2}} / sec</td>
				<td>{{toFixed meanRate 2}} / sec</td>
			</tr>
		{{/each}}
	</tbody>
</table>

